{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": [2, { "allowForLoopAfterthoughts": true }]
    },
    "import/no-duplicates": ["error", {"considerQueryString": true}]
}
